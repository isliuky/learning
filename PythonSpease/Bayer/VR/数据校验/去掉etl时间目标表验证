    doctors_interested,
    imaging_academy_registration_time,
    imaging_academy_academic_energy,
    imaging_academy_reading_content_last_6_mtd,
    imaging_academy_last_five_reading_time,
    imaging_academy_last_five_reading_type,
    ai_rep_code,
    imaging_academy_academic_energy_qtd,
    imaging_academy_academic_energy_rank,
    imaging_academy_total_reading_time_last_6_mtd,
    badge_num,
    certificate_num,
    imaging_academy_last_five_reading_content,
    topic_interststr,
    product_interest,
    system_interest
FROM "cn_cdp_dev"."analytical_martph_customer360"."customer_360_hcp";

